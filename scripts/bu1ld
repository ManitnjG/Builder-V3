#!/bin/bash
cd ~/work
export CCACHE_DIR=/tmp/ccache
export CCACHE_EXEC=$(which ccache)
export USE_CCACHE=1
ccache -M 120G
ccache -o compression=true
ccache -z
rm -rf frameworks/base
git clone https://github.com/ManitnjG/frameworks_base.git -b 13 frameworks/base
. build/envsetup.sh
lunch "$LUNCH_COMBO"
mka $TARGET -j8 & sleep 100m
